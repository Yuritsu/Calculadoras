        btnEsc.addActionListener(new ActionListener() {
            public void actionPerformed(ActionEvent e) {
                float escalar = Float.parseFloat(txtResEsc.getText());
                float[][] matriz = new float[3][3];
                matriz[0][0] = Float.parseFloat(txt11.getText());
                matriz[0][1] = Float.parseFloat(txt12.getText());
                matriz[0][2] = Float.parseFloat(txt13.getText());
                matriz[1][0] = Float.parseFloat(txt21.getText());
                matriz[1][1] = Float.parseFloat(txt22.getText());
                matriz[1][2] = Float.parseFloat(txt23.getText());
                matriz[2][0] = Float.parseFloat(txt31.getText());
                matriz[2][1] = Float.parseFloat(txt32.getText());
                matriz[2][2] = Float.parseFloat(txt33.getText());
                float[][] resultado = new float[3][3];
                for (int i = 0; i < 3; i++) {
                    for (int j = 0; j < 3; j++) {
                        resultado[i][j] = matriz[i][j] * escalar;
                    }
                }
                txtRes11.setText(String.valueOf(resultado[0][0]));
                txtRes12.setText(String.valueOf(resultado[0][1]));
                txtRes13.setText(String.valueOf(resultado[0][2]));
                txtRes21.setText(String.valueOf(resultado[1][0]));
                txtRes22.setText(String.valueOf(resultado[1][1]));
                txtRes23.setText(String.valueOf(resultado[1][2]));
                txtRes31.setText(String.valueOf(resultado[2][0]));
                txtRes32.setText(String.valueOf(resultado[2][1]));
                txtRes33.setText(String.valueOf(resultado[2][2]));
            }
        });

        JButton btnInvers = new JButton("Inversa");
        btnInvers.addActionListener(new ActionListener() {
            public void actionPerformed(ActionEvent e) {
                float[][] matriz1 = new float[3][3];
                matriz1[0][0] = Float.parseFloat(txt11.getText());
                matriz1[0][1] = Float.parseFloat(txt12.getText());
                matriz1[0][2] = Float.parseFloat(txt13.getText());
                matriz1[1][0] = Float.parseFloat(txt21.getText());
                matriz1[1][1] = Float.parseFloat(txt22.getText());
                matriz1[1][2] = Float.parseFloat(txt23.getText());
                matriz1[2][0] = Float.parseFloat(txt31.getText());
                matriz1[2][1] = Float.parseFloat(txt32.getText());
                matriz1[2][2] = Float.parseFloat(txt33.getText());

                float determinante;
                determinante = (matriz1[0][0]*matriz1[1][1]*matriz1[2][2])+(matriz1[1][0]*matriz1[2][1]*matriz1[0][2])+(matriz1[2][0]*matriz1[0][1] * matriz1[1][2]);
                determinante = determinante-(matriz1[0][2]*matriz1[1][1]*matriz1[2][0])-(matriz1[1][2]*matriz1[2][1]*matriz1[0][0])-(matriz1[2][2]*matriz1[0][1]*matriz1[1][0]);
                txtResDeter.setText(String.valueOf(determinante));
                
                if (determinante == 0) {
                	
                } else {
                    float[][] adjunta = new float[3][3];
                    adjunta[0][0] = matriz1[1][1] * matriz1[2][2] - matriz1[1][2] * matriz1[2][1];
                    adjunta[0][1] = matriz1[0][2] * matriz1[2][1] - matriz1[0][1] * matriz1[2][2];
                    adjunta[0][2] = matriz1[0][1] * matriz1[1][2] - matriz1[0][2] * matriz1[1][1];
                    adjunta[1][0] = matriz1[1][2] * matriz1[2][0] - matriz1[1][0] * matriz1[2][2];
                    adjunta[1][1] = matriz1[0][0] * matriz1[2][2] - matriz1[0][2] * matriz1[2][0];
                    adjunta[1][2] = matriz1[0][2] * matriz1[1][0] - matriz1[0][0] * matriz1[1][2];
                    adjunta[2][0] = matriz1[1][0] * matriz1[2][1] - matriz1[1][1] * matriz1[2][0];
                    adjunta[2][1] = matriz1[0][1] * matriz1[2][0] - matriz1[0][0] * matriz1[2][1];
                    adjunta[2][2] = matriz1[0][0] * matriz1[1][1] - matriz1[0][1] * matriz1[1][0];

                    float[][] resultado = new float[3][3];
                    for (int i = 0; i < 3; i++) {
                        for (int j = 0; j < 3; j++) {
                            resultado[i][j] = adjunta[i][j] / determinante;
                        }
                    } 
                txtRes11.setText(String.valueOf(resultado[0][0]));
                txtRes12.setText(String.valueOf(resultado[0][1]));
                txtRes13.setText(String.valueOf(resultado[0][2]));
                txtRes21.setText(String.valueOf(resultado[1][0]));
                txtRes22.setText(String.valueOf(resultado[1][1]));
                txtRes23.setText(String.valueOf(resultado[1][2]));
                txtRes31.setText(String.valueOf(resultado[2][0]));
                txtRes32.setText(String.valueOf(resultado[2][1]));
                txtRes33.setText(String.valueOf(resultado[2][2]));  
                }
                
            }
        });
        btnInvers.setForeground(new Color(30, 185, 69));
        btnInvers.setFont(new Font("Arial", Font.BOLD, 17));
        btnInvers.setBackground(Color.BLACK);
        btnInvers.setBounds(378, 285, 98, 50);
        contentPane.add(btnInvers);
    

        JButton btnpor = new JButton("x");
        btnpor.setForeground(Color.ORANGE);
        btnpor.setFont(new Font("Arial", Font.BOLD, 18));
        btnpor.setBackground(Color.BLACK);
        btnpor.setBounds(305, 285, 45, 50);
        contentPane.add(btnpor);
        btnpor.addActionListener(new ActionListener() {    
        public void actionPerformed(ActionEvent e) {
        	float[][] resultado = new float[3][3];
        	float[][] matriz1 = new float[3][3];
            matriz1[0][0] = Float.parseFloat(txt11.getText());
            matriz1[0][1] = Float.parseFloat(txt12.getText());
            matriz1[0][2] = Float.parseFloat(txt13.getText());
            matriz1[1][0] = Float.parseFloat(txt21.getText());
            matriz1[1][1] = Float.parseFloat(txt22.getText());
            matriz1[1][2] = Float.parseFloat(txt23.getText());
            matriz1[2][0] = Float.parseFloat(txt31.getText());
            matriz1[2][1] = Float.parseFloat(txt32.getText());
            matriz1[2][2] = Float.parseFloat(txt33.getText());
            float[][] matriz2 = new float[3][3];
            matriz2[0][0] = Float.parseFloat(txt211.getText());
            matriz2[0][1] = Float.parseFloat(txt212.getText());
            matriz2[0][2] = Float.parseFloat(txt213.getText());
            matriz2[1][0] = Float.parseFloat(txt221.getText());
            matriz2[1][1] = Float.parseFloat(txt222.getText());
            matriz2[1][2] = Float.parseFloat(txt223.getText());
            matriz2[2][0] = Float.parseFloat(txt231.getText());
            matriz2[2][1] = Float.parseFloat(txt232.getText());
            matriz2[2][2] = Float.parseFloat(txt233.getText());
            resultado[0][0] = (matriz1[0][0] * matriz2[0][0]) + (matriz1[0][1] * matriz2[1][0]) + (matriz1[0][2] * matriz2[2][0]);
            resultado[0][1] = (matriz1[0][0] * matriz2[0][1]) + (matriz1[0][1] * matriz2[1][1]) + (matriz1[0][2] * matriz2[2][1]);
            resultado[0][2] = (matriz1[0][0] * matriz2[0][2]) + (matriz1[0][1] * matriz2[1][2]) + (matriz1[0][2] * matriz2[2][2]);
            resultado[1][0] = (matriz1[1][0] * matriz2[0][0]) + (matriz1[1][1] * matriz2[1][0]) + (matriz1[1][2] * matriz2[2][0]);
            resultado[1][1] = (matriz1[1][0] * matriz2[0][1]) + (matriz1[1][1] * matriz2[1][1]) + (matriz1[1][2] * matriz2[2][1]);
            resultado[1][2] = (matriz1[1][0] * matriz2[0][2]) + (matriz1[1][1] * matriz2[1][2]) + (matriz1[1][2] * matriz2[2][2]);
            resultado[2][0] = (matriz1[2][0] * matriz2[0][0]) + (matriz1[2][1] * matriz2[1][0]) + (matriz1[2][2] * matriz2[2][0]);
            resultado[2][1] = (matriz1[2][0] * matriz2[0][1]) + (matriz1[2][1] * matriz2[1][1]) + (matriz1[2][2] * matriz2[2][1]);
            resultado[2][2] = (matriz1[2][0] * matriz2[0][2]) + (matriz1[2][1] * matriz2[1][2]) + (matriz1[2][2] * matriz2[2][2]);
            txtRes11.setText(String.valueOf(resultado[0][0]));
            txtRes12.setText(String.valueOf(resultado[0][1]));
            txtRes13.setText(String.valueOf(resultado[0][2]));
            txtRes21.setText(String.valueOf(resultado[1][0]));
            txtRes22.setText(String.valueOf(resultado[1][1]));
            txtRes23.setText(String.valueOf(resultado[1][2]));
            txtRes31.setText(String.valueOf(resultado[2][0]));
            txtRes32.setText(String.valueOf(resultado[2][1]));
            txtRes33.setText(String.valueOf(resultado[2][2]));
        }
        });
        
        JButton btndivision = new JButton("/");
        btndivision.addActionListener(new ActionListener() {
        	public void actionPerformed(ActionEvent e) {
            	float[][] matriz1 = new float[3][3];
                matriz1[0][0] = Float.parseFloat(txt11.getText());
                matriz1[0][1] = Float.parseFloat(txt12.getText());
                matriz1[0][2] = Float.parseFloat(txt13.getText());
                matriz1[1][0] = Float.parseFloat(txt21.getText());
                matriz1[1][1] = Float.parseFloat(txt22.getText());
                matriz1[1][2] = Float.parseFloat(txt23.getText());
                matriz1[2][0] = Float.parseFloat(txt31.getText());
                matriz1[2][1] = Float.parseFloat(txt32.getText());
                matriz1[2][2] = Float.parseFloat(txt33.getText());
                float[][] matriz2 = new float[3][3];
                matriz2[0][0] = Float.parseFloat(txt211.getText());
                matriz2[0][1] = Float.parseFloat(txt212.getText());
                matriz2[0][2] = Float.parseFloat(txt213.getText());
                matriz2[1][0] = Float.parseFloat(txt221.getText());
                matriz2[1][1] = Float.parseFloat(txt222.getText());
                matriz2[1][2] = Float.parseFloat(txt223.getText());
                matriz2[2][0] = Float.parseFloat(txt231.getText());
                matriz2[2][1] = Float.parseFloat(txt232.getText());
                matriz2[2][2] = Float.parseFloat(txt233.getText());
                float determinante;
                determinante = (matriz2[0][0]*matriz2[1][1]*matriz2[2][2])+(matriz2[1][0]*matriz2[2][1]*matriz2[0][2])+(matriz2[2][0]*matriz2[0][1] * matriz2[1][2]);
                determinante = determinante-(matriz2[0][2]*matriz2[1][1]*matriz2[2][0])-(matriz2[1][2]*matriz2[2][1]*matriz2[0][0])-(matriz2[2][2]*matriz2[0][1]*matriz2[1][0]);
                txtResDeter.setText(String.valueOf(determinante));
                
                if (determinante == 0) {
                	
                } else {
                	// Girar Mattrix 2
                    float[][] matriz2g = new float[3][3];
                    matriz2g[0][0] = matriz2[0][0];
                    matriz2g[1][0] = matriz2[0][1];
                    matriz2g[2][0] = matriz2[0][2];
                    matriz2g[0][1] = matriz2[1][0];
                    matriz2g[1][1] = matriz2[1][1];
                    matriz2g[2][1] = matriz2[1][2];
                    matriz2g[0][2] = matriz2[2][0];
                    matriz2g[1][2] = matriz2[2][1];
                    matriz2g[2][2] = matriz2[2][2];
                    // Resultado de la Inversa
                    float[][] adjunta = new float[3][3];
                    adjunta[0][0] = matriz2g[1][1] * matriz2g[2][2] - matriz2g[1][2] * matriz2g[2][1];
                    adjunta[0][1] = matriz2g[0][2] * matriz2g[2][1] - matriz2g[0][1] * matriz2g[2][2];
                    adjunta[0][2] = matriz2g[0][1] * matriz2g[1][2] - matriz2g[0][2] * matriz2g[1][1];
                    adjunta[1][0] = matriz2g[1][2] * matriz2g[2][0] - matriz2g[1][0] * matriz2g[2][2];
                    adjunta[1][1] = matriz2g[0][0] * matriz2g[2][2] - matriz2g[0][2] * matriz2g[2][0];
                    adjunta[1][2] = matriz2g[0][2] * matriz2g[1][0] - matriz2g[0][0] * matriz2g[1][2];
                    adjunta[2][0] = matriz2g[1][0] * matriz2g[2][1] - matriz2g[1][1] * matriz2g[2][0];
                    adjunta[2][1] = matriz2g[0][1] * matriz2g[2][0] - matriz2g[0][0] * matriz2g[2][1];
                    adjunta[2][2] = matriz2g[0][0] * matriz2g[1][1] - matriz2g[0][1] * matriz2g[1][0];
                    float[][] matrizinv = new float[3][3];
                    for (int i = 0; i < 3; i++) {
                        for (int j = 0; j < 3; j++) {
                        	matrizinv[i][j] = adjunta[i][j] / determinante;
                        }
                    }
                    float[][] resultado = new float[3][3];
                    for (int i = 0; i < 3; i++) {
                        for (int j = 0; j < 3; j++) {
               		 resultado[i][j] = matriz1[i][j] * matrizinv[i][j];    
               		 }
                  }
                txtRes11.setText(String.valueOf(resultado[0][0]));
                txtRes12.setText(String.valueOf(resultado[0][1]));
                txtRes13.setText(String.valueOf(resultado[0][2]));
                txtRes21.setText(String.valueOf(resultado[1][0]));
                txtRes22.setText(String.valueOf(resultado[1][1]));
                txtRes23.setText(String.valueOf(resultado[1][2]));
                txtRes31.setText(String.valueOf(resultado[2][0]));
                txtRes32.setText(String.valueOf(resultado[2][1]));
                txtRes33.setText(String.valueOf(resultado[2][2]));  
                }    
        	}
        });
        btndivision.setForeground(Color.ORANGE);
        btndivision.setFont(new Font("Arial", Font.BOLD, 18));
        btndivision.setBackground(Color.BLACK);
        btndivision.setBounds(305, 346, 45, 50);
        contentPane.add(btndivision);
        
        
        // Etiquetas
        JLabel lblNewLabel = new JLabel("Matriz 1");
        lblNewLabel.setForeground(Color.WHITE);
        lblNewLabel.setHorizontalAlignment(SwingConstants.CENTER);
        lblNewLabel.setFont(new Font("Sylfaen", Font.BOLD | Font.ITALIC, 23));
        lblNewLabel.setBounds(10, 11, 156, 28);
        contentPane.add(lblNewLabel);
        
        JLabel lblMatriz = new JLabel("Matriz 2");
        lblMatriz.setForeground(Color.WHITE);
        lblMatriz.setHorizontalAlignment(SwingConstants.CENTER);
        lblMatriz.setFont(new Font("Sylfaen", Font.BOLD | Font.ITALIC, 23));
        lblMatriz.setBounds(184, 11, 156, 28);
        contentPane.add(lblMatriz);
        
        JLabel lblResultado = new JLabel("Resultado");
        lblResultado.setForeground(Color.WHITE);
        lblResultado.setHorizontalAlignment(SwingConstants.CENTER);
        lblResultado.setFont(new Font("Sylfaen", Font.BOLD | Font.ITALIC, 23));
        lblResultado.setBounds(361, 11, 268, 28);
        contentPane.add(lblResultado);
    }
}
